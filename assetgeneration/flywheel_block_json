import re

colors = ["black", "gray", "light_gray", "white", "red", "orange", "yellow", "lime", "green", "cyan", "light_blue", "blue", "magenta", "purple", "pink"]
materials = ["steel", "brass"]

contents = """
{
	"parent": "extendedflywheels:block/flywheel/FLYWHEELDATAPATH",
	"loader": "forge:obj",
	"flip-v": true,
	"model": "extendedflywheels:models/block/flywheel/flywheel_shaftless.obj"
}
"""

for material in materials:
	for color in colors:
		filename = f"{color}_{material}_flywheel"
		output = re.sub("FLYWHEELDATAPATH", f"{filename}", contents)

		with open(f"output/{filename}_block.json", "w") as f:
			f.write(output)